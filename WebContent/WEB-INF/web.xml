<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <servlet>
    <servlet-name>AnalysisChoice</servlet-name>
    <servlet-class>servlets.AnalysisChoice</servlet-class>
  </servlet>
  
  <servlet>
    <servlet-name>Run</servlet-name>
    <servlet-class>servlets.Run</servlet-class>
  </servlet>
  
  <servlet>
    <servlet-name>StartupServlet</servlet-name>
    <servlet-class>startup.StartupServlet</servlet-class>
    <load-on-startup>0</load-on-startup>
  </servlet>
  
  
  
  <servlet-mapping>
    <servlet-name>AnalysisChoice</servlet-name>
    <url-pattern>/methods</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>Run</servlet-name>
    <url-pattern>/run</url-pattern>
  </servlet-mapping>
</web-app>